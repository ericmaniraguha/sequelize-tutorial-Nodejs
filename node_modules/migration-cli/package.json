    "babel-node": "^0.0.1-security",
    "babel-preset-es2015": "^6.24.1",
    "commander": "^3.0.1",
    "mysql2": "^1.7.0",
    "sequelize": "^5.18.1",
    "sequelize-cli": "^5.5.1"
  }
