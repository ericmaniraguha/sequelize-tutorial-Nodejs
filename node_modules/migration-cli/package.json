{
  "name": "migration-cli",
  "version": "1.1.1",
  "description": "a tool for using migrations in raw sql",
  "main": "src/migration-runner.js",
  "scripts": {
    "sequelize": "node ../../sequelize-cli/lib",
    "migration:migrate": "../../sequelize-cli db:migrate",
